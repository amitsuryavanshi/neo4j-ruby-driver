#!/usr/bin/env bash
set -euo pipefail
IFS=$'\n\t'
set -vx

gem install rake -v 12.3.3
gem install neo4j-rake_tasks
gem install hoe
gem install hoe-bundler
gem install hoe-gemspec
rake _12.3.3_ clean bundler:gemfile[,true] gem:spec
bundle update

# Do any other automated setup that you need to do here
